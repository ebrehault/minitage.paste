[buildout]
storage-mode=zeo

[instance]
zeo-client-cache-size = \${cache:zeo-client-cache-size}
zeo-client = on
zeo-address = \${hosts:zeo-address}
temporary-storage =
    <zeoclient>
        server \${hosts:zeo-address} 
        storage temp
        name zeostorage
        var \${buildout:directory}/var
    </zeoclient>
    mount-point /temp_folder
    container-class Products.TemporaryFolder.TemporaryContainer

